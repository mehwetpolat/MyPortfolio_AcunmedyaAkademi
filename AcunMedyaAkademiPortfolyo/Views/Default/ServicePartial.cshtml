@model List<AcunMedyaAkademiPortfolyo.Models.TblService>
<div id="services" class="section lb">
    <div class="container">
        <div class="section-title text-left">
            <h3>Hizmetlerim</h3>
            <p>İstenilen Projelerde Hızlı ve Düzenli Teslimat!</p>
        </div><!-- end title -->

        <div class="row">
            @foreach (var item in Model)
            {
                <div class="col-md-4">
                    <div class="services-inner-box">
                        <div class="ser-icon">
                            <!-- iconu buradan çek-->
                            <img src="@item.İmageUrl" class="img-fluid" alt="Image" width="40px":height="40px"/>
                        </div>
                        <h2>@item.Title</h2>
                        <p>@item.Description</p>
                    </div>
                </div>
            }
            <!-- end col -->
        </div><!-- end row -->
    </div><!-- end container -->
</div>
